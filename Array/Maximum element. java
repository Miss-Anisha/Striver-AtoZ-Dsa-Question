import java.util.* ;
import java.io.*; 

public class Solution {
        public static void main(String[]args){
            int arr[]={1,2,3,4,5};
            System.out.println(largestElement(arr,5));
            
    }

    static int largestElement(int[] arr, int n) {
      int max=arr[0];
      for(int i=0;i<n; i++){
        if(arr[i]>max){
          max=arr[i];
        }
      }
      return max;
    }
}
